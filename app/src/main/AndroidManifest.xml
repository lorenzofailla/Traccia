<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.lorenzofailla.vocalpacer">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/votrac"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".MainService" />

        <activity
            android:name=".Settings"
            android:label="@string/title_activity_settings" />

        <activity
            android:name=".Sessions"
            android:label="@string/title_activity_sessions" />

        <activity
            android:name=".SessionDetails"
            android:label="@string/title_activity_session_details"/>


        <activity
            android:name=".ShowSessionInMap"
            android:label="@string/title_activity_show_session_in_map" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyDi5QmXvRB6rfhukS0RM-t3BHGbZU-XJ3Q"/>

    </application>

</manifest>
